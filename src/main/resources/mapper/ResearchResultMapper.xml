<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.wang.dao.ResearchResultDao" >
    <resultMap id="BaseResultMap" type="com.wang.entity.ResearchResult" >
        <result column="name" property="name" jdbcType="VARCHAR" />
        <result column="result" property="result" jdbcType="VARCHAR" />
    </resultMap>
    <sql id="Base_Column_List" >
        did, name, result,image_url
    </sql>

    <select id="researchResult" resultType="com.wang.entity.ResearchResult"  parameterType="java.lang.Integer"  >
        select  <include refid="Base_Column_List" />
        from research_result
    </select>

    <select id="researchResultById" resultType="com.wang.entity.ResearchResult"  parameterType="java.lang.Integer"  >
        select  <include refid="Base_Column_List" />
        from research_result
        where did= #{did,jdbcType=INTEGER};
    </select>

</mapper>